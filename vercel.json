{
  "rewrites": [
    {
      "source": "/api/staging/(.*)",
      "destination": "https://discord-gpt-gql-api-staging.fly.dev/graphql"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://discord-gpt-gql-api-prod.fly.dev/graphql"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}